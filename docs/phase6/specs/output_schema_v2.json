{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Output Schema v2",
  "type": "object",
  "additionalProperties": false,
  "required": ["period", "metrics", "disclaimer_version"],
  "properties": {
    "period": {
      "type": "object",
      "additionalProperties": false,
      "required": ["start", "end"],
      "properties": {
        "start": {
          "type": "string",
          "format": "date"
        },
        "end": {
          "type": "string",
          "format": "date"
        }
      }
    },
    "metrics": {
      "type": "object",
      "additionalProperties": false,
      "required": ["cagr", "volatility", "max_drawdown"],
      "properties": {
        "cagr": {
          "type": "number"
        },
        "cumulative_return": {
          "type": "number"
        },
        "volatility": {
          "type": "number"
        },
        "max_drawdown": {
          "type": "number"
        }
      }
    },
    "disclaimer_version": {
      "type": "string"
    }
  }
}
